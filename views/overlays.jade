// modals
#sign-in-overlay.overlay.sign-in-overlay(data-module='overlay')
  .overlay-wrapper.sign-in-overlay-wrapper
    .sign-in-overlay-content-wrapper
      section.overlay-content.sign-in-overlay-content
        button.overlay-close.overlay-close-button-first(type='button') Close
        .sign-in-overlay-main-content
          .sign-in-overlay-title
            | Sign In
          .sign-in-overlay-body(data-module='signInAjax')
            form#loginForm1.sign-in-form1(method='POST', novalidate='novalidate', action='https://www.gucci.com/us/en/j_spring_security_check')
              input(type='hidden', name='CSRFToken', value='9e24a959-5971-4936-aa7d-40811f261da8')
              .input-content
                label.sign-in-label EMAIL ADDRESS
                input#j_username.input-login-email.input-text-form(type='email', name='j_username', required='required')
              .input-content
                label.sign-in-label
                  | PASSWORD
                  span.sign-in-form-help  (case sensitive)
                input#j_password.input-login-pwd.input-text-form(type='password', name='j_password', required='required')
              .error-message
              .stay-signed-in
                .custom-checkbox-white
                  input#_spring_security_remember_me(checked='', name='_spring_security_remember_me', type='checkbox')
                  label(for='_spring_security_remember_me')
                    span.custom-checkbox-grey-copy Stay signed in
              .call-actions
                button.continue-with-login.sign-button(type='submit') Sign In
                .recover-message
                  a.recover-password.overlay-open(href='#forgot-password-overlay') Forgot your password?
          // .sign-in-overlay-body
      // .overlay-content
    // .overlay-content-wrapper
  // .overlay-wrapper
